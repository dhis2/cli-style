{
  "name": "@dhis2/cli-style",
  "version": "3.3.4",
  "description": "The code and commit style for DHIS2.",
  "bin": {
    "d2-style": "bin/d2-style"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "./bin/d2-style js"
  },
  "author": "Viktor Varland",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/format": "^8.0.0",
    "@commitlint/lint": "^8.0.0",
    "@commitlint/load": "^8.0.0",
    "@commitlint/read": "^8.0.0",
    "@dhis2/cli-helpers-engine": "1.3.0",
    "babel-eslint": "^10.0.2",
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "fs-extra": "^8.1.0",
    "lint-staged": "^9.1.0",
    "perfy": "^1.1.5",
    "prettier": "^1.18.2",
    "semver": "^6.2.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "husky": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "./bin/d2-style commit check",
      "pre-commit": "./bin/d2-style validate js package --fix"
    }
  },
  "publishConfig": {
    "access": "public"
  }
